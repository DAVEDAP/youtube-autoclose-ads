{
  "manifest_version": 2,
  "name": "Youtube Autoclose Ads",
  "version": "1.0",
  "description": "Automatically close ads on youtube videos after an amount of seconds.",
  "author": "Gabriel Duarte",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["youtube-autoclose-ads.js"],
      "run_at": "document_start"
    }
  ],
  "icons": { 
    "16": "icon16.png",
    "48": "icon48.png",
   "128": "icon128.png" 
  },
  "homepage_url": "https://github.com/GabrielDuarteM/youtube-autoclose-ads/tree/master",
  "permissions": ["*://*.youtube.com/*", "tabs"]
}